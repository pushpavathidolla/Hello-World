pipeline {
    agent any
    tools {
        maven "maven 3.6.3"
    }
    
	stages{
	stage('SonarQube Analysis') 
        {
	steps {
        withSonarQubeEnv('SonarQube') {
        sh "mvn clean package sonar:sonar -Dsonar.sources=."
       }
      }
    }
    
    stage('Build') { 
     steps{    
      sh "mvn clean package"
      }
    }
